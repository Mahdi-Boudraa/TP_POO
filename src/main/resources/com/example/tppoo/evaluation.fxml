<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.front.EvaluationController">
    <children>
        <Label text="Evaluation des Objectifs" />
        <VBox fx:id="vboxEvaluation" spacing="10.0" />
        <HBox spacing="10.0">
            <Button fx:id="btnPrevious" text="Previous" onAction="#handlePrevious" />
            <Button fx:id="btnNext" text="Next" onAction="#handleNext" />
            <Button fx:id="btnFinish" text="Finish" onAction="#handleFinish" />
        </HBox>
    </children>
</VBox>
