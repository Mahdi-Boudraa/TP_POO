<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.front.ScoreObservationController">
    <children>
        <Label text="Total Score" />
        <Label fx:id="totalScoreLabel" />
        <Label text="Observation" />
        <TextArea fx:id="observationTextArea" />
        <Button text="Submit" onAction="#handleSubmit" />
    </children>
    <padding>
        <Insets top="10" right="10" bottom="10" left="10" />
    </padding>
</VBox>
